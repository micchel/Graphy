<div class="graphy-container container-fluid">
    <div class="graphy-row row">
        <div class="sidebar-container col-lg-3">
            <%= render 'shared/user-sidebar' %>
        </div>

        <div class="main-content col-lg-9 bg-light overflow-auto">
            <header class="main-header border-bottom-main">
                <h3 class="main-header__text"><i class="fas fa-camera pr-1"></i>気になるフォトグラファー</h3>
            </header>
            <div class="container-fluid">
                <div class="row">
                    <%= render partial: "shared/photographer-card", collection: @photographers, as: "photographer" %>
                </div>
            </div>
        </div>

    </div>
</div>